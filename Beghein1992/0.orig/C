FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      C;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 950;

boundaryField
{
    fixedWalls
    {
        type            zeroGradient;
    }

    leftside
    {
        type            fixedValue;
        value           uniform 900.;
    }

    rightside
    {
        type            fixedValue;
        value           uniform 1000.;
    }

}


